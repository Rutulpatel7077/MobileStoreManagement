<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="477.0" prefWidth="759.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="mobilestoremanagement.AllUsersController">
    <stylesheets>
        <URL value="@Mobilecss.css" />
    </stylesheets>
   <children>
      <VBox layoutY="80.0" prefHeight="259.0" prefWidth="763.0">
         <children>
            <TableView fx:id="userTable" prefHeight="267.0" prefWidth="763.0">
              <columns>
                <TableColumn fx:id="userIdColumn" prefWidth="104.0" text="User ID" />
                <TableColumn fx:id="firstNameColumn" prefWidth="138.0" text="First Name" />
                  <TableColumn fx:id="lastNameColumn" prefWidth="137.0" text="Last Name" />
                  <TableColumn fx:id="birthdayColumn" prefWidth="147.0" text="Birthdate" />
                  <TableColumn fx:id="phoneColumn" prefWidth="236.0" text="PhoneNumber" />
              </columns>
            </TableView>
         </children>
      </VBox>
      <Button fx:id="editButton" layoutX="633.0" layoutY="367.0" mnemonicParsing="false" onAction="#editButtonPushed" prefHeight="45.0" prefWidth="116.0" text="Edit User" />
      <Button layoutX="525.0" layoutY="367.0" mnemonicParsing="false" onAction="#addButtonPushed" prefHeight="45.0" prefWidth="101.0" text="Add New" />
      <Button layoutX="7.0" layoutY="367.0" mnemonicParsing="false" onAction="#GotoMobileStore" prefHeight="45.0" prefWidth="191.0" text="Access Mobile Store" />
      <Button layoutX="212.0" layoutY="367.0" mnemonicParsing="false" onAction="#ProfitViewButtonPushed" prefHeight="45.0" prefWidth="191.0" text="Profit Report" />
   </children>
</AnchorPane>
